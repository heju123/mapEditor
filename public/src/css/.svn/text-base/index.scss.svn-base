.index-layout-tile-2x2 {
    width: 471px;
    height: 471px;
}
.index-layout-tile-1x1 {
    width: 224px;
    height: 224px;
}
.index-layout-tile-2x1 {
    width: 471px;
    height: 224px;
}

.index-box{
    margin: 8px;
    -webkit-box-shadow: 1px 2px 2px rgba(69,69,69,.55);
    -moz-box-shadow: 1px 2px 2px rgba(69,69,69,.55);
    box-shadow: 1px 2px 2px rgba(69,69,69,.55);
    background: #ffffff;

    .index-top-bg{
        width:100%;
        height:252px;
        background: url(../images/index/dashboardpeople.png) center no-repeat;
    }
    .index-box1-sta1{
        height:108px;
        width:100%;
        position: relative;
        margin-top: 18px;

        .sta1-top-icon{
            width:100%;
            height: 86px;
        }

        .sta-icon-1{
            background: url(../images/index/sta_icon_1.png) bottom no-repeat;
        }
        .sta-icon-2{
            background: url(../images/index/sta_icon_2.png) bottom no-repeat;
        }
        .sta1-label{
            font-size: 14px;
            color: #ffffff;
            text-align: center;
        }
        .sta1-num{
            width: 48px;
            height: 32px;
            line-height: 32px;
            position: absolute;
            right: 5px;
            top: 1px;
            background-color: #0572CE;
            font-size: 16px;
            color: #ffffff;
            border-radius: 20px;
            border: 2px solid #ffffff;
            text-align: center;
        }
    }

    .index-box1-sta2-outer{
        background: url(../images/index/myteam_watermark@2x.png) no-repeat scroll 25px 17px / 75% auto transparent;

        .index-box1-sta2{
            width:100%;
            height:45px;
            line-height:45px;
            overflow: hidden;
            border-bottom: 1px solid #525868;
            color: #ffffff;

            .sta2-label{
                float: left;
                font-size: 14px;
                margin-left: 10px;
            }
            .sta2-num{
                float: right;
                font-size: 26px;
                margin-right: 10px;
            }
        }

        .index-box1-sta2-nob{
            border-bottom : none;
        }
    }

    .index-box1-bottom{
        color: #ABB5B9;
        text-align: center;
        font-size: 12px;
        background-color: #434a5e;
        height:38px;
        line-height: 38px;
        margin-top: 12px;
    }

    .index-box2-sta1{
        height: 48px;
        line-height: 48px;
        border-bottom: 1px solid #d5dfe5;
        overflow: hidden;
        padding: 0px 10px 0px 10px;

        .sta1-dot{
            width: 10px;
            height: 10px;
            margin-top: 19px;
            border-radius: 5px;
            float: left;
        }
        .sta1-bg-green{
            background-color: #76B417;
        }
        .sta1-bg-blue{
            background-color: #0572CE;
        }
        .sta1-bg-red{
            background-color: #ff0000;
        }
        .sta1-label{
            color: #333333;
            margin-left: 10px;
            font-size: 14px;
            float: left;
        }
        .sta1-num{
            font-size: 26px;
            float: right;
        }
        .sta1-color-green{
            color: #76B417;
        }
        .sta1-color-blue{
            color: #0572CE;
        }
        .sta1-color-red{
            color: #ff0000;
        }
    }

    .index-box2-bottom{
        height:38px;
        line-height:38px;
        margin-top: 2px;
        color: #ffffff;
        background-color: #414557;
        text-align: center;
    }

    .index-box3-sta1{
        height:27px;
        line-height:27px;
        padding:0px 10px;

        .sta1-label{
            text-align: right;;
        }
        .sta1-num{
            font-weight: bold;
            text-align: right;
        }
        .sta1-prog{
            width:100%;
            height: 19px;
            background-color: #E2EED9;
            position: relative;

            .sta1-prog-val{
                height: 19px;
                background: #87BE66;
            }
        }
    }

    .index-box4-chart{
        width:100%;
        height:100%;
        border-top: 40px solid transparent;
        margin-top: -40px;
    }
}

.index-box1{
    filter:alpha(opacity=90); /* IE */
    -moz-opacity:0.9; /* 老版Mozilla */
    opacity: 0.9; /* 支持opacity的浏览器*/
    background: #313749;
}

.index-title{
    font-size: 18px;
    color: #ffffff;
}