}
a:hover {
    text-decoration: underline;
}

button, input, select, textarea {
    font-family: inherit;
    font-size: 100%;
    margin: 0;
}

input, button, select, textarea,a {
    outline: none !important;
}
input::-moz-focus-inner,button::-moz-focus-inner,select::-moz-focus-inner,textarea::-moz-focus-inner,a::-moz-focus-inner {
    border: 0;
}

