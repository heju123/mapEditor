.hj-normal-button{
    background: #e4e8ea;
    height: 30px;
    line-height: 30px;
    color: #000;
    border:1px solid #c4ced7;
    padding: 0px 28px 0px 28px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    text-align: center;
    cursor: pointer;
}
.hj-normal-button:hover{
    color: #0572ce;
    background-color: #f7f8f9;
    border-color: #c4ced7;
}
.hj-normal-button:hover .btn-icon-before:before{
    color: #0572ce;
}
.hj-normal-button:active {
    color: #fff;
    background-color: #0572ce;
}
.hj-normal-button:active .btn-icon-before:before{
    color: #fff;
}
.hj-normal-button .btn-icon-before{
    padding: 0px 6px 0px 6px;
    position: relative;
    top: 1px;
}
.hj-normal-button .btn-icon-before:before{
    display: inline-block;
    color: #878c90;
}
.hj-btn-icon-1:before{
    content: "\e607";
}
.hj-btn-icon-2:before{
    content: "\e608";
}

@media (max-width: 1024px) {
    .hj-normal-button .btn-label {
        display: none;
    }
}

