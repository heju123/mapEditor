.dialog{
    width: 300px;
    min-width: 200px;
    height: auto;
    -webkit-box-shadow: 1px 1px 10px 0 rgba(0,0,0,0.45);
    -moz-box-shadow: 1px 1px 10px 0 rgba(0,0,0,0.45);
    box-shadow: 1px 1px 10px 0 rgba(0,0,0,0.45);
    background: #f9f9f9;
    border: 1px solid #c4ced7;
    border-radius: 1px;
    display: none;
    position: fixed;
    z-index: 100000;

    .dialog-title{
        padding: 10px;
        background-color: #f0f0f0;
        border-width: 0 0 1px 0;
        border-style: solid;
        border-bottom-color: #c4ced7;
        font-size: 18px;
        color: #252525;
        font-weight: normal;
        position: relative;
        height: 40px;

        .dialog-close{
            position: absolute;
            top:5px;
            right:5px;
            width:32px;
            height:32px;
            font-family: "Alta Icon Font";
            font-size: 16px;
            cursor: pointer;
            border: 1px solid transparent;

            &:hover{
                color: #0572ce;
                background-color: #f7f8f9;
                border: 1px solid #c4ced7;
                box-shadow: none;
            }
            &:before{
                width:32px;
                height:32px;
                text-align: center;
                line-height: 32px;
                content: "\e608";
                display: inline-block;
            }
        }
    }

    .dialog-content{
        background: url("../images/home/texture.png");
        color: #333;
        padding:10px;
        overflow: hidden;
        height:100%;
        border-top: 40px solid transparent;
        margin-top: -40px;
        overflow-y: auto;
    }
}

.img-preview-box-outer{
    display: none;

    .img-preview-box{
        position: fixed;
        left:50%;
        top:50%;
        z-index:100000;
        -webkit-box-shadow: 1px 1px 8px 7px #828282;
        -moz-box-shadow: 1px 1px 8px 7px #828282;
        box-shadow: 1px 1px 8px 7px #828282;

        img{
        }

        .img-preview-closebtn{
            position: absolute;
            top: -15px;
            right: -15px;
            width:30px;
            height:30px;
            background: url("../images/common/imgPreview/fancybox.png") -40px 0;
            cursor: pointer;
        }
    }

    .img-preview-box-bg{
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgb(119, 119, 119);
        filter:alpha(opacity=70); /*支持 IE 浏览器*/
        -moz-opacity:0.70; /*支持 FireFox 浏览器*/
        opacity:0.70; /*支持 Chrome, Opera, Safari 等浏览器*/
        -ms-user-select: none;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
        z-index: 99999;
        cursor: pointer;
    }
}

.tree-box{
    width:100%;
    height:100%;
    overflow-y: auto;

    .tree-content{
        .tree-node{
            white-space: nowrap;

            .ope-btn{
                width:16px;
                height:16px;
                display: inline-block;
                font-family: "Alta Icon Font";
                font-size: 16px;
                color: #878c90;
                cursor: pointer;
                -webkit-transition: -webkit-transform 200ms;
                -moz-transition: -moz-transform 200ms;
                -ms-transition: -ms-transform 200ms;
                -o-transition: -o-transform 200ms;
                transition: transform 200ms;

                &:before{
                    content: "\e603";
                    display: inline-block;
                    width:16px;
                    height:16px;
                }
                &:hover{
                    color: #85bbe7;
                }
            }

            .ope-btn-open{
                -webkit-transform: rotate(45deg);
                -moz-transform: rotate(45deg);
                -ms-transform: rotate(45deg);
                -o-transform: rotate(45deg);
                transform: rotate(45deg);
            }

            .tree-icon{
                display: inline-block;
                width:16px;
                height:16px;
                margin-left:5px;
                background: url("../images/common/tree/folder.png") center no-repeat;
                position: relative;
                top: 2px;
                cursor: pointer;
            }

            .tree-title-outer{
                .tree-title{
                    border: 1px solid transparent;
                    background-color: transparent;
                    color: #045fab;
                    cursor: pointer;
                    margin-left: 5px;
                    text-overflow: ellipsis;
                    overflow: hidden;

                    &:hover{
                        border: 1px dashed #4f4f4f;
                    }
                }
            }

            .tree-children{
                padding-left: 15px;
                overflow: hidden;
                transform-origin: top;
                -webkit-transform: scaleY(0);
                -moz-transform: scaleY(0);
                -ms-transform: scaleY(0);
                -o-transform: scaleY(0);
                transform: scaleY(0);
                position: absolute;
                filter:alpha(opacity=0); /*支持 IE 浏览器*/
                -moz-opacity:0.0; /*支持 FireFox 浏览器*/
                opacity:0.0; /*支持 Chrome, Opera, Safari 等浏览器*/

                $trsiTime : 300ms;
                -webkit-transition: -webkit-transform $trsiTime,opacity $trsiTime;
                -moz-transition: -moz-transform $trsiTime,opacity $trsiTime;
                -ms-transition: -ms-transform $trsiTime,opacity $trsiTime;
                -o-transition: -o-transform $trsiTime,opacity $trsiTime;
                transition: transform $trsiTime,opacity $trsiTime;
            }
            .tree-children-show{
                position: static;
                -webkit-transform: scaleY(1);
                -moz-transform: scaleY(1);
                -ms-transform: scaleY(1);
                -o-transform: scaleY(1);
                transform: scaleY(1);
                filter:alpha(opacity=100); /*支持 IE 浏览器*/
                -moz-opacity:1.0; /*支持 FireFox 浏览器*/
                opacity:1.0; /*支持 Chrome, Opera, Safari 等浏览器*/
            }
        }

        .tree-node-sel>.tree-title-outer>.tree-title{
            border: 1px solid #bfd4e5;
            background-color: #e4f0fa;
            font-weight: bold;

            &:hover{
                background-color: #cfe8fc;
                border: 1px solid #045fab;
            }
        }
    }
}